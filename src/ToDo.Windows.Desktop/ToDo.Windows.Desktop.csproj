<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
    <OutputType>WinExe</OutputType>
    <TargetFramework>net5.0-windows10.0.19041.0</TargetFramework>
    <TargetPlatformMinVersion>10.0.17763.0</TargetPlatformMinVersion>
    <RootNamespace>ToDo</RootNamespace>
    <ApplicationManifest>app.manifest</ApplicationManifest>
    <Platforms>x86;x64;arm64</Platforms>
    <RuntimeIdentifiers>win10-x86;win10-x64;win10-arm64</RuntimeIdentifiers>
    <UseWinUI>true</UseWinUI>
    <DefineConstants>$(DefineConstants);WINUI</DefineConstants>
  </PropertyGroup>
  <ItemGroup>
    <Manifest Include="$(ApplicationManifest)" />
  </ItemGroup>
  <ItemGroup>
    <PackageReference Include="Microsoft.WindowsAppSDK" Version="1.0.1" />
    <PackageReference Include="Microsoft.Windows.SDK.BuildTools" Version="10.0.22000.197" />
    <PackageReference Include="Microsoft.Extensions.Logging.Console" Version="6.0.0" />
    <PackageReference Include="Uno.Core.Extensions.Logging.Singleton" Version="4.0.1" />
  </ItemGroup>
  <ItemGroup>
    <FrameworkReference Update="Microsoft.Windows.SDK.NET.Ref" RuntimeFrameworkVersion="10.0.18362.22" />
    <FrameworkReference Update="Microsoft.Windows.SDK.NET.Ref" TargetingPackVersion="10.0.18362.22" />
  </ItemGroup>
  <ItemGroup>
    <PackageReference Include="Uno.WinUI" Version="4.1.8" />
    <PackageReference Include="Uno.Core" Version="4.0.1" />
    <PackageReference Include="Uno.Material.WinUI" Version="1.2.0" />
    <PackageReference Include="Uno.Toolkit.WinUI.Material" Version="1.2.1" />
    <PackageReference Include="Uno.Toolkit.WinUI" Version="1.2.1" />
    <PackageReference Include="Uno.SourceGenerationTasks" Version="4.0.0" />
  </ItemGroup>
  <ItemGroup>
    <PackageReference Include="Uno.Extensions.Configuration" Version="1.2.0-dev.308" />
    <PackageReference Include="Uno.Extensions.Core" Version="1.2.0-dev.308" />
    <PackageReference Include="Uno.Extensions.Hosting" Version="1.2.0-dev.308" />
    <PackageReference Include="Uno.Extensions.Hosting.WinUI" Version="1.2.0-dev.308" />
    <PackageReference Include="Uno.Extensions.Http" Version="1.2.0-dev.308" />
    <PackageReference Include="Uno.Extensions.Localization.WinUI" Version="1.2.0-dev.308" />
    <PackageReference Include="Uno.Extensions.Logging.Serilog" Version="1.2.0-dev.308" />
    <PackageReference Include="Uno.Extensions.Logging.WinUI" Version="1.2.0-dev.308" />
    <PackageReference Include="Uno.Extensions.Navigation.Toolkit.WinUI" Version="1.2.0-dev.308" />
    <PackageReference Include="Uno.Extensions.Navigation.WinUI" Version="1.2.0-dev.308" />
    <PackageReference Include="Uno.Extensions.Navigation" Version="1.2.0-dev.308" />
    <PackageReference Include="Uno.Extensions.Serialization" Version="1.2.0-dev.308" />
    <PackageReference Include="Uno.Extensions.Reactive" Version="1.2.0-dev.308" />
    <PackageReference Include="Uno.Extensions.Reactive.WinUI" Version="1.2.0-dev.308" />
  </ItemGroup>
  <ItemGroup>
    <ProjectReference Include="..\ToDo\ToDo.csproj" />
  </ItemGroup>
  <Import Project="..\ToDo.UI\ToDo.UI.projitems" Label="Shared" />
</Project>