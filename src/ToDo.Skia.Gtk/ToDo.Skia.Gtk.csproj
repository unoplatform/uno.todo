<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
    <OutputType Condition="'$(Configuration)'=='Release'">WinExe</OutputType>
    <OutputType Condition="'$(Configuration)'=='Debug'">Exe</OutputType>
    <TargetFramework>net5.0</TargetFramework>
    <DefineConstants>$(DefinedConstants);WINUI</DefineConstants>
  </PropertyGroup>
  <ItemGroup Condition="exists('..\ToDo.UWP')">
    <EmbeddedResource Include="..\ToDo.UWP\Package.appxmanifest" LogicalName="Package.appxmanifest" />
    <Content Include="..\ToDo.UWP\Assets\StoreLogo.png" Link="Assets\StoreLogo.png" />
    <Content Include="Assets\Fonts\uno-fluentui-assets.ttf" />
  </ItemGroup>
  <ItemGroup>
    <UpToDateCheckInput Include="..\ToDo.UI\**\*.xaml" />
  </ItemGroup>
  <ItemGroup>
    <PackageReference Include="CommunityToolkit.Mvvm" Version="7.1.2" />
    <PackageReference Include="Microsoft.Extensions.Configuration" Version="6.0.1" />
    <PackageReference Include="Microsoft.Extensions.Configuration.Binder" Version="6.0.0" />
    <PackageReference Include="Microsoft.Extensions.Http" Version="6.0.0" />
    <PackageReference Include="Microsoft.Extensions.Logging" Version="6.0.0" />
    <PackageReference Include="Microsoft.Extensions.Logging.Console" Version="6.0.0" />
    <PackageReference Include="Refit" Version="6.3.2" />
    <PackageReference Include="Refit.HttpClientFactory" Version="6.3.2" />
    <PackageReference Include="Uno.WinUI.Skia.Gtk" Version="4.2.0-dev.543" />
    <PackageReference Include="Uno.WinUI.RemoteControl" Version="4.2.0-dev.543" Condition="'$(Configuration)'=='Debug'" />
    <PackageReference Include="Uno.UI.Adapter.Microsoft.Extensions.Logging" Version="4.1.9" />
    <PackageReference Include="Uno.Core" Version="4.0.1" />
    <PackageReference Include="Uno.Material.WinUI" Version="1.4.0-dev.30" />
    <PackageReference Include="Uno.Toolkit.WinUI.Material" Version="1.4.0-dev.3" />
    <PackageReference Include="Uno.Toolkit.WinUI" Version="1.4.0-dev.3" />
    <PackageReference Include="Uno.SourceGenerationTasks" Version="4.0.2" />
  </ItemGroup>
  <ItemGroup>
    <PackageReference Include="Uno.Extensions.Configuration" Version="1.2.0-dev.362" />
    <PackageReference Include="Uno.Extensions.Core" Version="1.2.0-dev.362" />
    <PackageReference Include="Uno.Extensions.Hosting" Version="1.2.0-dev.362" />
    <PackageReference Include="Uno.Extensions.Hosting.WinUI" Version="1.2.0-dev.362" />
    <PackageReference Include="Uno.Extensions.Http" Version="1.2.0-dev.362" />
    <PackageReference Include="Uno.Extensions.Localization.WinUI" Version="1.2.0-dev.362" />
    <PackageReference Include="Uno.Extensions.Logging.Serilog" Version="1.2.0-dev.362" />
    <PackageReference Include="Uno.Extensions.Logging.WinUI" Version="1.2.0-dev.362" />
    <PackageReference Include="Uno.Extensions.Navigation.Toolkit.WinUI" Version="1.2.0-dev.362" />
    <PackageReference Include="Uno.Extensions.Navigation.WinUI" Version="1.2.0-dev.362" />
    <PackageReference Include="Uno.Extensions.Navigation" Version="1.2.0-dev.362" />
    <PackageReference Include="Uno.Extensions.Serialization" Version="1.2.0-dev.362" />
    <PackageReference Include="Uno.Extensions.Reactive" Version="1.2.0-dev.362" />
    <PackageReference Include="Uno.Extensions.Reactive.WinUI" Version="1.2.0-dev.362" />
  </ItemGroup>
  <ItemGroup>
    <ProjectReference Include="..\ToDo\ToDo.csproj" />
  </ItemGroup>
  <Import Project="..\ToDo.UI\ToDo.UI.projitems" Label="Shared" />
</Project>