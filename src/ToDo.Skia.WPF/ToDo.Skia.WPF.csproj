    <PackageReference Include="Uno.WinUI.Skia.Wpf" Version="4.1.9" />
    <PackageReference Include="Uno.Core" Version="4.0.1" />
    <PackageReference Include="Uno.Material.WinUI" Version="1.3.1" />
    <PackageReference Include="Uno.Toolkit.WinUI.Material" Version="1.3.0" />
