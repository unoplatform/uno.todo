<Project Sdk="Microsoft.NET.Sdk.Web">
  <PropertyGroup>
    <OutputType>Exe</OutputType>
    <TargetFramework>net5.0</TargetFramework>
    <NoWarn>NU1701</NoWarn>
    <DefineConstants>$(DefineConstants);WINUI</DefineConstants>
  </PropertyGroup>
  <PropertyGroup Condition="'$(Configuration)'=='Debug'">
    <MonoRuntimeDebuggerEnabled>true</MonoRuntimeDebuggerEnabled>
    <DefineConstants>$(DefineConstants);TRACE;DEBUG</DefineConstants>
    <DebugType>portable</DebugType>
    <DebugSymbols>true</DebugSymbols>
    <!--
		IL Linking is disabled in Debug configuration.
		When building in Release, see https://platform.uno/docs/articles/features/using-il-linker-webassembly.html
		-->
    <WasmShellILLinkerEnabled>false</WasmShellILLinkerEnabled>
  </PropertyGroup>
  <PropertyGroup Condition="'$(Configuration)'=='Debug' or '$(IsUiAutomationMappingEnabled)'=='True'">
    <IsUiAutomationMappingEnabled>True</IsUiAutomationMappingEnabled>
    <DefineConstants>$(DefineConstants);USE_UITESTS</DefineConstants>
  </PropertyGroup>
  <ItemGroup>
    <Content Include="Assets\SplashScreen.png" />
  </ItemGroup>
  <ItemGroup>
    <UpToDateCheckInput Include="..\ToDo.UI\**\*.xaml" />
  </ItemGroup>
  <ItemGroup>
    <EmbeddedResource Include="WasmCSS\Fonts.css" />
    <EmbeddedResource Include="WasmScripts\AppManifest.js" />
  </ItemGroup>
  <ItemGroup>
    <LinkerDescriptor Include="LinkerConfig.xml" />
  </ItemGroup>
  <ItemGroup>
    <!--
		This item group is required by the project template because of the
		new SDK-Style project, otherwise some files are not added automatically.

		You can safely remove this ItemGroup completely.
		-->
    <None Include="Program.cs" />
    <None Include="LinkerConfig.xml" />
    <None Include="wwwroot\web.config" />
  </ItemGroup>
  <ItemGroup>
    <PackageReference Include="Microsoft.Extensions.Configuration" Version="6.0.1" />
    <PackageReference Include="Microsoft.Extensions.Configuration.Binder" Version="6.0.0" />
    <PackageReference Include="Microsoft.Extensions.Http" Version="6.0.0" />
    <PackageReference Include="Microsoft.Windows.Compatibility" Version="6.0.0" />
    <PackageReference Include="Microsoft.Extensions.Logging" Version="6.0.0" />
    <PackageReference Include="Refit" Version="6.3.2" />
    <PackageReference Include="Refit.HttpClientFactory" Version="6.3.2" />
    <PackageReference Include="Uno.Extensions.Logging.WebAssembly.Console" Version="1.3.0-dev.1" />
    <PackageReference Include="Uno.WinUI.WebAssembly" Version="4.2.0-dev.543" />
    <PackageReference Include="Uno.WinUI.RemoteControl" Version="4.2.0-dev.543" Condition="'$(Configuration)'=='Debug'" />
    <PackageReference Include="Uno.UI.Adapter.Microsoft.Extensions.Logging" Version="4.1.9" />
    <PackageReference Include="Uno.Wasm.Bootstrap" Version="4.0.0-dev.103" />
    <PackageReference Include="Uno.Wasm.Bootstrap.DevServer" Version="3.3.1" />
    <PackageReference Include="Uno.Core" Version="4.0.1" />
    <PackageReference Include="Uno.Material.WinUI" Version="1.4.0-dev.30" />
    <PackageReference Include="Uno.Toolkit.WinUI.Material" Version="1.4.0-dev.3" />
    <PackageReference Include="Uno.Toolkit.WinUI" Version="1.4.0-dev.3" />
    <PackageReference Include="Uno.SourceGenerationTasks" Version="4.0.2" />
  </ItemGroup>
  <ItemGroup>
    <PackageReference Include="Uno.Extensions.Configuration" Version="1.2.0-dev.342" />
    <PackageReference Include="Uno.Extensions.Core" Version="1.2.0-dev.342" />
    <PackageReference Include="Uno.Extensions.Hosting" Version="1.2.0-dev.342" />
    <PackageReference Include="Uno.Extensions.Hosting.WinUI" Version="1.2.0-dev.342" />
    <PackageReference Include="Uno.Extensions.Http" Version="1.2.0-dev.342" />
    <PackageReference Include="Uno.Extensions.Localization.WinUI" Version="1.2.0-dev.342" />
    <PackageReference Include="Uno.Extensions.Logging.Serilog" Version="1.2.0-dev.342" />
    <PackageReference Include="Uno.Extensions.Logging.WinUI" Version="1.2.0-dev.342" />
    <PackageReference Include="Uno.Extensions.Navigation.Toolkit.WinUI" Version="1.2.0-dev.342" />
    <PackageReference Include="Uno.Extensions.Navigation.WinUI" Version="1.2.0-dev.342" />
    <PackageReference Include="Uno.Extensions.Navigation" Version="1.2.0-dev.342" />
    <PackageReference Include="Uno.Extensions.Serialization" Version="1.2.0-dev.342" />
    <PackageReference Include="Uno.Extensions.Reactive" Version="1.2.0-dev.342" />
    <PackageReference Include="Uno.Extensions.Reactive.WinUI" Version="1.2.0-dev.342" />
  </ItemGroup>
  <ItemGroup>
    <ProjectReference Include="..\ToDo\ToDo.csproj" />
  </ItemGroup>
  <Import Project="..\ToDo.UI\ToDo.UI.projitems" Label="Shared" Condition="Exists('..\ToDo.UI\ToDo.UI.projitems')" />
</Project>