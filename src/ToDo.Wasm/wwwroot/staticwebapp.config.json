{
  "navigationFallback": {
    "rewrite": "/index.html",
    "exclude": [
      "*.{css,js}",
      "*.{png}",
      "*.{c,h,wasm,clr,pdb,dat,txt,json}"
    ]
  },
  "mimeTypes": {
    ".json": "text/json"
  },
  "routes": [
    {
      "route": "/authentication-callback",
      "rewrite": "/authentication/login-callback.htm"
    },
    {
      "route": "/package_*",
      "headers": {
        "cache-control": "public, immutable, max-age=31536000"
      }
    },
    {
      "route": "/*.ttf",
      "headers": {
        "cache-control": "public, immutable, max-age=31536000"
      }
    },
    {
      "route": "/*",
      "headers": {
        "cache-control": "must-revalidate, max-age=3600"
      }
    }
  ]
}